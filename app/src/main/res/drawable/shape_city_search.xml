<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 填充颜色 -->
    <solid android:color="@color/grayLine" />
    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="1dp"
        android:color="@color/gray2" />
    <!-- 矩形的圆角半径 -->
    <corners android:radius="18dp" />
    <padding
        android:bottom="3dp"
        android:left="0dp"
        android:right="0dp"
        android:top="3dp" />

</shape>